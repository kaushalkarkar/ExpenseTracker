@model IEnumerable<WebApplication7.Context.expensecate>

@{
    ViewBag.Title = "CategoryList";
}

<h2>CategoryList</h2>

<p>
    @Html.ActionLink("Create New", "ExpenseCategory")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpenseCategory)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <th>
            @Html.DisplayFor(model => item.CategoryID)
        </th>

        <td>
            @Html.DisplayFor(modelItem => item.ExpenseCategory)
        </td>
        <td>


            @Html.ActionLink("Delete", "Delete", new { id = item.CategoryID })
        </td>
    </tr>
}

</table>
